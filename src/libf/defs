		'bput_varm_short-3'	=> 'in:intToSizeTOffsetArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_short-4'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_short-5'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_short-6'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
	'bput_varm_int' => '2:3:4:5:6',
		'bput_varm_int-2'	=> 'in:OffsetIndexIn',
		'bput_varm_int-3'	=> 'in:intToSizeTOffsetArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_int-4'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_int-5'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_int-6'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
	'bput_varm_float' => '2:3:4:5:6',
		'bput_varm_float-2'	=> 'in:OffsetIndexIn',
		'bput_varm_float-3'	=> 'in:intToSizeTOffsetArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_float-4'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_float-5'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_float-6'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
	'bput_varm_double' => '2:3:4:5:6',
		'bput_varm_double-2'	=> 'in:OffsetIndexIn',
		'bput_varm_double-3'	=> 'in:intToSizeTOffsetArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_double-4'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_double-5'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_double-6'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
	'bput_varm_longlong' => '2:3:4:5:6',
		'bput_varm_longlong-2'	=> 'in:OffsetIndexIn',
		'bput_varm_longlong-3'	=> 'in:intToSizeTOffsetArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_longlong-4'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_longlong-5'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',
		'bput_varm_longlong-6'	=> 'in:intToSizeTArr:ncmpixVardim(*v1,*v2-1)',


	'rename_att' => '2:3:4',
		'rename_att-2'		=> 'in:OffsetIndexIn',
		'rename_att-3'		=> 'in:addnull',
		'rename_att-4'		=> 'in:addnull',
	'rename_dim' => '2:3',
		'rename_dim-2'		=> 'in:OffsetIndexIn',
		'rename_dim-3'		=> 'in:addnull',
	'rename_var' => '2:3',
		'rename_var-2'		=> 'in:OffsetIndexIn',
		'rename_var-3'		=> 'in:addnull'
);

$build_prototypes = 1;
$buildMakefile = 0;
$prototype_header_file = "mpifnetcdf.h";
$argsneedcast{'const MPI_Offset []'} = '(const MPI_Offset *)(ARG)';
$argsneedcast{'nc_type *'} = '(nc_type *)(ARG)';
$argsneedcast{'nc_type'} = '(nc_type)(ARG)';

